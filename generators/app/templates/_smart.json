{
  "global": {
    "color": {
      "grey": "#dfe0e1",
      "white": "#ffffff",
      "black": "#333333",
      "solid": "#4e4e4e",
      "edward": "#a3abac",
      "primary": "#64c4d4"
    },
    "files": {
      "dest": "assets",
      "source": "src",
      "concatenator": "-"
    },
    "default": {
      "none": "none",
      "width": 512,
      "height": 512,
      "zoom": 1,
      "stroke_width": 26,
      "transform": "translate(0)"
    },
    "bot": {
      "name": "iAyeBot",
      "email": "iayebot@websemantics.ca",
      "url": "http:\/\/github.com\/iayebot",
      "img": "https:\/\/avatars1.githubusercontent.com\/u\/18201639?v=3&amp;s=40"
    }
  },
  "template": [
    {
      "description": "A collection of logos with mascot or mascot and text",
      "source": "{{ global.files.source }}\/logo.template",
      "dest": "{{ global.files.dest }}",
      "data":[
        "original",
        "flat",
        "solid",
        "solid-white",
        "lite",
        "outline",
        "outline-lite",
        "outline-white"
      ],
      "svg": {
        "dest": "svg"
      },
      "png": {
        "dest": "png",
        "scale": {
          "width": [
            200, 400
          ],
          "height": [
            32, 64, 256
          ]
        },
        "retina": true
      },
      "icon": {
        "dest": "icon",
        "source": "{{ global.files.dest }}\/png\/original-mascot-h256@2x.png",
        "config": {
          "appName": "{{ package.name }}",
          "appDescription": "{{ package.description }}",
          "developerName": "{{ package.author }}",
          "developerURL": "{{ package.homepage }}",
          "background": "{{ global.color.white }}",
          "path": "{{ global.files.dest }}\/icon",
          "url": "{{ package.homepage }}",
          "display": "standalone",
          "orientation": "portrait",
          "version": 1,
          "logging": false,
          "online": false,
          "replace": true
        }
      },
      "sprite": {
        "dest": "sprite",
        "source": "{{ global.files.dest }}\/svg\/*.svg",
        "config": {
          "mode": {
            "view": {
              "bust": false,
              "render": {
                "css": true
              }
            },
            "symbol": true
          }
        }
      }
    }
  ],
  "html": {
    "dest": ".",
    "template": "{{ global.files.source }}\/index.template",
    "filename": "index.html",
    "title": "iBuild Logos .. ",
    "description": "{{ package.description }}",
    "github_url": "{{& package.repository }}",
    "link": {
      "text": "Visit Website \u00bb",
      "url": "{{& package.repository.url }}"
    },
    "generated_by": {
      "text": "Generated by,",
      "name": "{{ global.bot.name }}",
      "url": "{{& global.bot.url }}",
      "img": "{{& global.bot.img }}"
    }
  },
  "data": {
    "default": {
      "name": "",
      "value": {
        "flat": true,
        "text": false,
        "original_mascot": true,
        "zoom": "{{ global.default.zoom }}",
        "width": "{{ global.default.width }}",
        "height": "{{ global.default.height }}",
        "body_stroke": "{{ global.default.none }}",
        "border_stroke": "{{ global.color.black }}",
        "stroke_width": "{{ global.default.stroke_width }}",
        "body_fill": "{{ global.default.none }}",
        "dot_fill": "{{ global.color.black }}",
        "text_fill": "{{ global.color.black }}",
        "text_dot_fill": "{{ global.color.black }}",
        "background_fill": "{{ global.color.white }}",
        "primary_fill": "{{ global.color.primary}}",
        "grey_fill": "{{ global.color.grey }}",
        "transform": "{{ global.default.transform }}"
      }
    },
    "mascot": {
      "children": [
        "default"
      ],
      "value": {

      }
    },
    "text": {
      "children": [
        "default"
      ],
      "value": {
        "text": true,
        "transform": "translate(-66)",
        "width": 1985
      }
    },
    "original": {
      "children": [
        "mascot",
        "text"
      ],
      "value": {
        "flat": false
      }
    },
    "flat": {
      "children": [
        "mascot",
        "text"
      ],
      "value": {
        "background_fill": "{{ global.default.none }}",
        "dot_fill": "{{ global.color.edward }}",
        "body_fill": "{{ global.color.primary }}",
        "body_stroke": "{{ global.color.primary }}",
        "text_fill": "{{ global.color.edward }}",
        "text_dot_fill": "{{ global.color.primary }}"
      }
    },
    "solid": {
      "children": [
        "mascot",
        "text"
      ],
      "value": {
        "dot_fill": "{{ global.color.solid }}",
        "body_fill": "{{ global.color.solid }}",
        "body_stroke": "{{ global.color.solid }}",
        "text_fill": "{{ global.color.solid }}",
        "text_dot_fill": "{{ global.color.solid }}"
      }
    },
    "solid-white": {
      "children": [
        "mascot",
        "text"
      ],
      "value": {
        "background_fill": "{{ global.default.none }}",
        "dot_fill": "{{ global.color.white }}",
        "body_fill": "{{ global.color.white }}",
        "body_stroke": "{{ global.color.white }}",
        "text_fill": "{{ global.color.white }}",
        "text_dot_fill": "{{ global.color.white }}"
      }
    },
    "lite": {
      "children": [
        "mascot",
        "text"
      ],
      "value": {
        "background_fill": "{{ global.default.none }}",
        "dot_fill": "{{ global.color.edward }}",
        "body_fill": "{{ global.color.edward }}",
        "body_stroke": "{{ global.color.edward }}",
        "text_fill": "{{ global.color.edward }}",
        "text_dot_fill": "{{ global.color.edward }}"
      }
    },
    "outline": {
      "children": [
        "mascot",
        "text"
      ],
      "value": {
        "background_fill": "{{ global.default.none }}",
        "dot_fill": "{{ global.color.solid }}",
        "body_fill": "{{ global.default.none }}",
        "body_stroke": "{{ global.color.solid }}",
        "text_fill": "{{ global.color.solid }}",
        "text_dot_fill": "{{ global.color.solid }}"
      }
    },
    "outline-lite": {
      "children": [
        "mascot",
        "text"
      ],
      "value": {
        "background_fill": "{{ global.default.none }}",
        "dot_fill": "{{ global.color.edward }}",
        "body_fill": "{{ global.default.none }}",
        "body_stroke": "{{ global.color.edward }}",
        "text_fill": "{{ global.color.edward }}",
        "text_dot_fill": "{{ global.color.edward }}"
      }
    },
    "outline-white": {
      "children": [
        "mascot",
        "text"
      ],
      "value": {
        "background_fill": "{{ global.default.none }}",
        "dot_fill": "{{ global.color.white }}",
        "body_fill": "{{ global.default.none }}",
        "body_stroke": "{{ global.color.white }}",
        "text_fill": "{{ global.color.white }}",
        "text_dot_fill": "{{ global.color.white }}"
      }
    }
  }
}
